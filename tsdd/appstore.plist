<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>items</key>
    <array>
        <dict>
            <key>assets</key>
            <array>
                <!-- 1. 指向 IPA 文件的下载链接 -->
                <dict>
                    <key>kind</key>
                    <string>software-package</string>
                    <key>url</key>
                    <string>https://github.com/tsdaodao/tsddipa/releases/download/v1/TangSengDaoDaoappstore.ipa</string> <!-- 替换为你的 IPA 链接 -->
                </dict>
                <!-- 可选：应用图标（57x57 或 512x512） -->
                <dict>
                    <key>kind</key>
                    <string>display-image</string>
                    <key>url</key>
                    <string>https://yourdomain.com/icon.png</string>
                </dict>
            </array>
            <!-- 2. 应用元数据 -->
            <key>metadata</key>
            <dict>
                <key>bundle-identifier</key>
                <string>com.xinbida.shanxin</string> <!-- 必须与 Xcode 中的 Bundle ID 一致 -->
                <key>bundle-version</key>
                <string>1.0.0</string> <!-- 版本号 -->
                <key>kind</key>
                <string>software</string>
                <key>title</key>
                <string>Your App Name</string> <!-- 应用名称 -->
            </dict>
        </dict>
    </array>
</dict>
</plist>
